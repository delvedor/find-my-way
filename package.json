  },
  "homepage": "https://github.com/delvedor/find-my-way#readme",
  "devDependencies": {
    "@types/node": "^11.9.4",
    "benchmark": "^2.1.4",
    "coveralls": "^3.0.2",
    "pre-commit": "^1.2.2",
    "request": "^2.88.0",
    "standard": "^12.0.1",
    "tap": "^12.5.3",
    "typescript": "^3.3.3"
  },
  "dependencies": {
